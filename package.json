{
  "name": "choclacode",
  "version": "1.4.2",
  "description": "Choclacode official website v1",
  "main": "app.js",
  "author": "Saqib <saqib@choclacode.eu.org> (http://choclacode.eu.org/)",
  "license": "ISC",
  "engines": {
    "node": "15.x"
  },
  "scripts": {
    "start": "node .",
    "serve": "npx concurrently \"npm run serve:app\" \"npm run watch\"",
    "serve:app": "npx nodemon",
    "build": "npx concurrently \"npm run build:js\" \"npm run build:sass\"",
    "build:js": "webpack",
    "build:sass": "npx node-sass resources/sass/main.scss -o public/styles --output-style compressed",
    "watch": "npx concurrently \"npm run watch:js\" \"npm run watch:sass\"",
    "watch:js": "webpack -w",
    "watch:sass": "npx node-sass -w resources/sass/main.scss -o public/styles --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/choclacode/choclacode.git"
  },
  "bugs": {
    "url": "https://github.com/choclacode/choclacode/issues"
  },
  "homepage": "http://choclacode.eu.org/",
  "keywords": [
    "choclacode",
    "saqib.ml"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "marked": "^1.2.7",
    "mongoose": "^5.10.12",
    "pug": "^3.0.2",
    "socket.io": "^3.0.1"
  },
  "devDependencies": {
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0"
  }
}
